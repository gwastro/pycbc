strain_segments_dict = strain.StrainSegments.from_cli_multi_ifos(opt, 
  				         strain_dict, opt.instruments)
with ctx:
    fft.from_cli(opt)

    # Set some often used variables for easy access
    flow = opt.low_frequency_cutoff
