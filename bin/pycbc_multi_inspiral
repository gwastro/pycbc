    plt.plot(x,y,'r')
    roh_coh_for_hist = coherent_snr(snr,np.arange(len(snr[ifo])),0,projection_matrix)
    plt.hist(roh_coh_for_hist.real**2,bins=100,normed=True)
    plt.title("roh_coh2")
